package com.example.demo.model;

import org.springframework.data.relational.core.mapping.Column;

import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;
import jakarta.persistence.Table;

@Entity
@Table(name = "estimatecost")
public class Estimate {

    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private int id;
	private String ProjectId;
    private String ProjectName;
    private String PlumberCost;
    private String LabourCost;
    private String MaterialCost;
    private String electricianCost;
    
    
    public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getProjectId() {
		return ProjectId;
	}
	public void setProjectId(String projectId) {
		ProjectId = projectId;
	}
	public String getProjectName() {
		return ProjectName;
	}
	public void setProjectName(String projectName) {
		ProjectName = projectName;
	}
	public String getPlumberCost() {
		return PlumberCost;
	}
	public void setPlumberCost(String plumberCost) {
		PlumberCost = plumberCost;
	}
	
	
	public String getLabourCost() {
		return LabourCost;
	}
	public void setLabourCost(String labourCost) {
		LabourCost = labourCost;
	}
	public String getMaterialCost() {
		return MaterialCost;
	}
	public void setMaterialCost(String materialCost) {
		MaterialCost = materialCost;
	}

	public String getelectricianCost() {
		return electricianCost;
	}
	public void setelectricianCost( String electriciancost) {
	        electricianCost = electriciancost;
	}

}