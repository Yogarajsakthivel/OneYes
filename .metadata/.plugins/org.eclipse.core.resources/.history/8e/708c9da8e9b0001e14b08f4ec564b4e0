package com.example.demo.model;

import org.springframework.data.relational.core.mapping.Column;

import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;
import jakarta.persistence.Table;

@Entity
@Table(name = "estimatecost")
public class Estimate {

    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private int id;
	private int ProjectId;
    private String ProjectName;
    private int PlumberCost;
    private int LabourCost;
    private int MaterialCost;
    private double electricianCost;
    
    
    public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public int getProjectId() {
		return ProjectId;
	}
	public void setProjectId(int projectId) {
		ProjectId = projectId;
	}
	public String getProjectName() {
		return ProjectName;
	}
	public void setProjectName(String projectName) {
		ProjectName = projectName;
	}
	public int getPlumberCost() {
		return PlumberCost;
	}
	public void setPlumberCost(int plumberCost) {
		PlumberCost = plumberCost;
	}
	
	
	public int getLabourCost() {
		return LabourCost;
	}
	public void setLabourCost(int labourCost) {
		LabourCost = labourCost;
	}
	public int getMaterialCost() {
		return MaterialCost;
	}
	public void setMaterialCost(int materialCost) {
		MaterialCost = materialCost;
	}

	public double getelectricianCost() {
		return electricianCost;
	}
	public void setelectricianCost( double electricianCost) {
		this.electricianCost = electricianCost;
	}

}